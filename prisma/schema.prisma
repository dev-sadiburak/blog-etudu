
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = process.env.MONGO_CONN_STR
}

model Post {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  title     String
  content   String
  labels    String[]
  createdAt DateTime @default(now())
}